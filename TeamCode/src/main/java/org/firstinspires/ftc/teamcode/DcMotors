package org.firstinspires.ftc.teamcode;

import com.qualcomm.robotcore.hardware.DcMotor;


public class DcMotors {

        DcMotor[] innerMotors;

        public DcMotors make(DcMotor... motors) {
            this.innerMotors = motors;
            return this;
        }
        
        public void setPower(double num) {
            for (int i = 0; i < innerMotors.length; i++) {
                innerMotors[i].setPower(num);
            }
        }

        /* public void run(String fnName, String... args) {
            for (int i = 0; i < innerMotors.length; i++) {
                DcMotor.getClass().getMethod(fnName);
            }
        } */
}